module sixJetsFilter = EtMinCaloJetCountFilter {
    InputTag src = iterativeCone5CaloJets
    uint32 minNumber = 6
    double etMin = 15.0
}

module twoBJetsFilter = JetTagCountFilter {
    InputTag src = trackCountingHighEffJetTags
    double minDiscriminator = 2.0
    double minJetEt = 30.0
    double maxJetEta = 2.5
    uint32 minNumber = 2
}

sequence topFullyHadronicJets  = { sixJetsFilter  }
sequence topFullyHadronicBJets = { twoBJetsFilter }
